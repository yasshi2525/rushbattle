{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended",
    "prettier/@typescript-eslint"
  ],
  "plugins": ["@typescript-eslint", "es"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "sourceType": "module"
  },
  "env": { "browser": true, "node": true, "es6": true },
  "rules": {
    "no-console": "error",
    "no-restricted-properties": [
      "error",
      { "object": "Array", "property": "find" }
    ],
    "es/no-array-of": "error",
    "es/no-array-from": "error",
    "es/no-object-is": "error",
    "es/no-object-assign": "error",
    "es/no-object-entries": "error",
    "es/no-object-values": "error",
    "es/no-promise": "error",
    "es/no-set": "error",
    "es/no-map": "error",
    "es/no-spread-elements": "error"
  }
}
